project {
  requires += rpc
  Define_Custom(rpcgen) {
    command              = rpcgen
    commandflags         = -C
    pch_postrule         = 1
    inputext             = .x
    source_pre_extension = _svc, _clnt
    source_outputext     = .c
    header_outputext     = .h
  }
}
