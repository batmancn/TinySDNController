project {
  // Allow users to specify .mpb files in their project.  This gives the
  // opportunity to see them in IDE's and to install them using
  // prj_install.pl.
  Define_Custom(MPB) {
    automatic_in  = 0
    automatic_out = 0
    inputext      = .mpb
  }
}
