project: wxcore {
  specific(prop:windows) {                                  
    Debug::lit_libs   += wx$(WX_PORT)$(WX_VERSION)$(WX_UNICODE)d_stc
    Release::lit_libs += wx$(WX_PORT)$(WX_VERSION)$(WX_UNICODE)_stc
  } else {
    lit_libs += wx_$(WX_PORT)$(WX_UNICODE)_stc$(WX_VERSION)
  }
}
